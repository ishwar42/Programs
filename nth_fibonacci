#include <stdio.h>

int fib(int n)
{
    if (n <= 1)
        return n;
    else
        return fib(n-1) + fib(n-2);
}

int main()
{
    int n, i, x;
    printf("Enter the number:\n");
    scanf("%d", &n);
    printf("Fibonacci Series is:\n");
    for (i = 0; i < n; i++)
        x=fib(i);
    printf("%d ",x);
    return 0;
}
